{
  "name": "@salesflare/nylas",
  "version": "4.3.0",
  "description": "A NodeJS wrapper for the Nylas REST API for email, contacts, and calendar.",
  "main": "lib/nylas.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint \"./src/**/*.js\"",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"./src/**/*.js\"",
    "build": "babel src --presets babel-preset-env --out-dir lib",
    "prepublish": "npm run build",
    "prepare": "npm run build",
    "watch": "nodemon -w src --exec npm run build"
  },
  "keywords": [
    "email",
    "contacts",
    "calendar",
    "nylas"
  ],
  "author": "Nylas, Inc.",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "1.3.4",
    "async": "2.6.1",
    "backoff": "2.5.0",
    "bluebird": "^3.5.1",
    "clone": "2.1.2",
    "express-session": "^1.15.6",
    "request": "^2.88.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.5.0",
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nylas/nylas-nodejs.git"
  }
}
